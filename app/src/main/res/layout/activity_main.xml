<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <FrameLayout
        android:id="@+id/id_frameLayout"
        android:layout_width="match_parent"
        android:layout_height="650dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/id_bottomNav_3">



    </FrameLayout>

    <ImageView
        android:id="@+id/id_bottomNav_3"
        android:layout_width="52dp"
        android:layout_height="61dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_frameLayout"
        app:srcCompat="@drawable/ic_add_task_24px" />

    <ImageView
        android:id="@+id/id_bottomNav_2"
        android:layout_width="52dp"
        android:layout_height="61dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toStartOf="@+id/id_bottomNav_3"
        app:layout_constraintTop_toBottomOf="@+id/id_frameLayout"
        app:srcCompat="@drawable/ic_account_balance_wallet_24px" />

    <ImageView
        android:id="@+id/id_bottomNav_1"
        android:layout_width="52dp"
        android:layout_height="61dp"
        app:layout_constraintEnd_toStartOf="@+id/id_bottomNav_2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_frameLayout"
        app:srcCompat="@drawable/ic_account_balance_24px" />

    <ImageView
        android:id="@+id/id_bottomNav_5"
        android:layout_width="52dp"
        android:layout_height="61dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/id_bottomNav_4"
        app:layout_constraintTop_toBottomOf="@+id/id_frameLayout"
        app:srcCompat="@drawable/ic_build_24px" />

    <ImageView
        android:id="@+id/id_bottomNav_4"
        android:layout_width="52dp"
        android:layout_height="61dp"
        android:layout_marginStart="36dp"
        app:layout_constraintStart_toEndOf="@+id/id_bottomNav_3"
        app:layout_constraintTop_toBottomOf="@+id/id_frameLayout"
        app:srcCompat="@drawable/ic_account_circle_24px" />


</androidx.constraintlayout.widget.ConstraintLayout>



